<div class="mx-auto">
  <%= month_calendar events: bookings do |date, booking_list| %>
    <%= is_available?(date) %>
    <% booking_list.each do |booking| %>
      <% if booking.status == "approved" %>
        <div class="bg-warning rounded text-center mt-2">
          <p>Déjà réservé</p>
        </div>
      <% end %>
    <% end %>
  <% end %>
</div>